<label for="{{label}}">{{label}}</label>
{{#if (eq cardinality "vector")}}
<input
  .key=${ {{key}} }
  name="{{label}}"
  type="range"
  min="0"
  .value=${ {{variable_to_read}} }
  @change=${(e: any) => {
     this._{{variable_to_change}} = this._{{variable_to_change}}.map((v, idx) => {
      if ({{key}} === idx) {
        v = e.target.value;
      }
      return v;
    });
  } }
>
{{else}}
<input
  name="{{label}}"
  type="range"
  min="0"
  .value=${ {{variable_to_read}} }
  @change=${(e: any) => { {{variable_to_change}} = e.detail.value; } }
>
{{/if}}